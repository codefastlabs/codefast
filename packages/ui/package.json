{
  "name": "@codefast/ui",
  "version": "0.0.3",
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./avatar": {
      "types": "./src/avatar.tsx",
      "import": "./dist/avatar.mjs",
      "require": "./dist/avatar.js"
    },
    "./aspect-ratio": {
      "types": "./src/aspect-ratio.tsx",
      "import": "./dist/aspect-ratio.mjs",
      "require": "./dist/aspect-ratio.js"
    },
    "./alert-dialog": {
      "types": "./src/alert-dialog.tsx",
      "import": "./dist/alert-dialog.mjs",
      "require": "./dist/alert-dialog.js"
    },
    "./dropdown-menu": {
      "types": "./src/dropdown-menu.tsx",
      "import": "./dist/dropdown-menu.mjs",
      "require": "./dist/dropdown-menu.js"
    },
    "./breadcrumb": {
      "types": "./src/breadcrumb.tsx",
      "import": "./dist/breadcrumb.mjs",
      "require": "./dist/breadcrumb.js"
    },
    "./badge": {
      "types": "./src/badge.tsx",
      "import": "./dist/badge.mjs",
      "require": "./dist/badge.js"
    },
    "./alert": {
      "types": "./src/alert.tsx",
      "import": "./dist/alert.mjs",
      "require": "./dist/alert.js"
    },
    "./button": {
      "types": "./src/button.tsx",
      "import": "./dist/button.mjs",
      "require": "./dist/button.js"
    },
    "./utils": {
      "types": "./src/utils.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js"
    },
    "./styles.css": "./dist/styles.css",
    "./tailwind.config": "./tailwind.config.ts"
  },
  "files": [
    "dist",
    "src",
    "tailwind.config.ts"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup --watch",
    "generate:component": "turbo gen react-component",
    "lint": "eslint . --max-warnings 0"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "cva": "1.0.0-beta.1",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@codefast/eslint-config": "workspace:*",
    "@codefast/typescript-config": "workspace:*",
    "@turbo/gen": "^1.12.5",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^18.0",
    "react-dom": "^18.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
