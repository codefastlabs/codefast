{
  "name": "codefast",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "pnpm -r exec rm -rf .next dist",
    "build": "turbo run build",
    "build:docs": "turbo run build --filter=@app/docs...",
    "build:packages": "turbo run build --filter=@codefast*/*...",
    "clean": "turbo clean",
    "preclean:cache": "pnpm store prune",
    "clean:cache": "turbo clean",
    "clean:modules": "pnpm -r exec rm -rf node_modules",
    "postclean:modules": "rm -rf node_modules pnpm-lock.yaml",
    "clean:next": "pnpm -r exec rm -rf .next",
    "codefast": "codefast",
    "deploy:preview": "vercel deploy --archive=tgz",
    "deps:audit": "pnpm audit",
    "deps:audit:fix": "pnpm audit --fix",
    "deps:install": "pnpm install",
    "deps:licenses": "pnpm licenses list",
    "deps:outdated": "pnpm outdated --recursive",
    "predeps:reinstall": "pnpm clean",
    "deps:reinstall": "pnpm deps:install",
    "postdeps:reinstall": "pnpm build:packages",
    "deps:update": "pnpm update --latest --interactive --recursive",
    "predev": "pnpm clean:next",
    "dev": "turbo run dev --concurrency 11",
    "predev:docs": "pnpm clean:next",
    "dev:docs": "turbo run dev --concurrency 11 --filter=@app/docs...",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "precommit": "lint-staged",
    "prepare": "simple-git-hooks",
    "release": "changeset",
    "release:canary:enter": "changeset pre enter canary",
    "release:canary:exit": "changeset pre exit",
    "start": "turbo run start",
    "start:docs": "turbo run start --filter=@app/docs...",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "test:coverage:ci": "turbo run test:coverage:ci",
    "test:watch": "turbo run test:watch",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.5",
    "@codefast/cli": "workspace:*",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "eslint": "^9.32.0",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "simple-git-hooks": "^2.13.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
