{
  "name": "@codefast/docs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "clean": "rm -rf .next && rm -rf .turbo && rm -rf storybook-static && rm -rf node_modules",
    "dev": "concurrently -k -p \"[{name}]\" -n \"next,storybook\" -c \"cyan,yellow\" \"pnpm run dev-next\" \"pnpm run dev-storybook\"",
    "dev-next": "next dev --turbo",
    "dev-storybook": "storybook dev -p 6006 --no-open --quiet",
    "lint": "next lint --fix --max-warnings 0",
    "start": "next start",
    "start-storybook": "serve storybook-static",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@codefast/ui": "workspace:*",
    "@hookform/resolvers": "^3.4.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@tanstack/react-table": "^8.17.0",
    "date-fns": "^3.6.0",
    "embla-carousel-autoplay": "^8.1.3",
    "jotai": "^2.8.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.379.0",
    "next": "14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "recharts": "^2.12.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.5",
    "@codefast/eslint-config": "workspace:*",
    "@faker-js/faker": "^8.4.1",
    "@next/eslint-plugin-next": "^14.2.3",
    "@storybook/addon-essentials": "^8.1.2",
    "@storybook/addon-interactions": "^8.1.2",
    "@storybook/addon-links": "^8.1.2",
    "@storybook/addon-themes": "^8.1.2",
    "@storybook/blocks": "^8.1.2",
    "@storybook/nextjs": "^8.1.2",
    "@storybook/react": "^8.1.2",
    "@storybook/test": "^8.1.2",
    "@types/eslint": "^8.56.10",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-turbo": "^1.13.3",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.38",
    "serve": "^14.2.3",
    "storybook": "^8.1.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  }
}
