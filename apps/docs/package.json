{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "clean": "rm -rf .next && rm -rf .turbo && rm -rf storybook-static && rm -rf node_modules",
    "dev": "concurrently -k -p \"[{name}]\" -n \"next,storybook\" -c \"cyan,yellow\" \"pnpm run dev-next\" \"pnpm run dev-storybook\"",
    "dev-next": "next dev --turbo",
    "dev-storybook": "pnpm storybook",
    "lint": "next lint --fix --max-warnings 0",
    "start": "next start",
    "start-storybook": "serve storybook-static",
    "storybook": "storybook dev -p 6006 --no-open --quiet"
  },
  "dependencies": {
    "@codefast/hooks": "workspace:*",
    "@codefast/third-parties": "workspace:*",
    "@codefast/ui": "workspace:*",
    "@hookform/resolvers": "3.9.0",
    "@radix-ui/react-icons": "1.3.0",
    "@tanstack/react-table": "8.20.5",
    "date-fns": "3.6.0",
    "embla-carousel-autoplay": "8.2.1",
    "lodash": "4.17.21",
    "lucide-react": "0.438.0",
    "next": "14.2.8",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.0",
    "recharts": "alpha",
    "recoil": "0.7.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.8.0",
    "@codefast/config-tailwind": "workspace:*",
    "@codefast/eslint-config": "workspace:*",
    "@faker-js/faker": "9.0.0",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/addon-interactions": "8.2.9",
    "@storybook/addon-links": "8.2.9",
    "@storybook/addon-themes": "8.2.9",
    "@storybook/blocks": "8.2.9",
    "@storybook/nextjs": "8.2.9",
    "@storybook/react": "8.2.9",
    "@storybook/test": "8.2.9",
    "@types/lodash": "4.17.7",
    "@types/node": "22.5.4",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "chromatic": "11.7.1",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.8",
    "eslint-config-turbo": "2.1.1",
    "eslint-plugin-storybook": "0.8.0",
    "postcss": "8.4.45",
    "serve": "14.2.3",
    "storybook": "8.2.9",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4"
  }
}
